void createList(N *A[MAX]){
  int src,dest,choice;
  do{
    printf("Enter the src and dest \n");
    scanf("%d %d",&src,&dest);

    addEdge(V,src,dest);
    addEdge(V,src,dest);

    printf("Do you want to add one more edge\n");
    scanf("%d",&choice);
  }while(choice);
}
